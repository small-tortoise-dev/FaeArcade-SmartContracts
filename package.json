    "ton-core": "^0.53.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "ton-core": "^0.53.0"
  },
  "packageManager": "pnpm@8.0.0"
